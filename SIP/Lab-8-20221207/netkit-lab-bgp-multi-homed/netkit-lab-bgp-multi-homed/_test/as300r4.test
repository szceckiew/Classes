#!/bin/sh

sleep 1m

# Dump information about RIP entries
expect -c "
spawn telnet localhost ripd
expect \"Password:*\"
send \"zebra\\r\"
expect \"ripd>*\"
send \"terminal length 0\\r\"
expect \"ripd>*\"
send \"show ip rip\\r\"
expect \"ripd>*\"
send \"exit\\r\"
send_user \"\\r\"" | sed -n '20,$p' | head -n -3 | awk '{print $1,$2}' | sort

route -n


sleep 10

touch /hostlab/as300r4.testdone
halt

